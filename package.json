{
  "name": "booklibrary",
  "version": "0.1.0",
  "main": "src/index.ts",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "build": "swc src -d dist",
    "build:watch": "swc src -d dist -w",
    "start:dev": "dotenvx run --env-file=.env.dev -- node ./dist/index.js",
    "start:test": "dotenvx run --env-file=.env.test -- node ./dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts, --fix",
    "prettier-check": "prettier --check '**/*.{ts,json}'",
    "prettier-format": "prettier --write '**/*.{ts,json}'"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.2",
    "@dotenvx/dotenvx": "^0.44.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/mongodb": "^8.0.0",
    "@fastify/one-line-logger": "^1.2.0",
    "@fastify/swagger": "^8.8.0",
    "@fastify/swagger-ui": "^3.0.0",
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.19.2",
    "fastify-plugin": "^4.5.0",
    "firebase-admin": "^12.0.0",
    "mongodb": "^6.0.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.68",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "bson-objectid": "^2.0.4",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "tsx": "^3.12.7",
    "typescript": "5.1.6",
    "vitest": "^0.32.4"
  }
}
